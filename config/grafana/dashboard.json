{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Active Tasks",
      "type": "stat",
      "gridPos": { "x": 0, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "conductor_tasks_total{status=\"executing\"}",
          "legendFormat": "Active"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 3 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "title": "Completed Tasks (24h)",
      "type": "stat",
      "gridPos": { "x": 4, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "increase(conductor_tasks_total{status=\"done\"}[24h])",
          "legendFormat": "Completed"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "green" }
        }
      }
    },
    {
      "title": "Failed Tasks (24h)",
      "type": "stat",
      "gridPos": { "x": 8, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "increase(conductor_tasks_total{status=\"failed\"}[24h])",
          "legendFormat": "Failed"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" }
        }
      }
    },
    {
      "title": "Total Cost (24h)",
      "type": "stat",
      "gridPos": { "x": 12, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "increase(conductor_cost_total_dollars[24h])",
          "legendFormat": "Cost"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 2
        }
      }
    },
    {
      "title": "Avg Task Duration",
      "type": "stat",
      "gridPos": { "x": 16, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "conductor_task_duration_seconds_avg",
          "legendFormat": "Duration"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Total Tokens (24h)",
      "type": "stat",
      "gridPos": { "x": 20, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "increase(conductor_tokens_total[24h])",
          "legendFormat": "Tokens"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "title": "Tasks by Status",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "conductor_tasks_total",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "title": "Subtasks by Status",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "conductor_subtasks_total",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "title": "Token Usage Over Time",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(conductor_tokens_input[5m]) * 60",
          "legendFormat": "Input Tokens/min"
        },
        {
          "expr": "rate(conductor_tokens_output[5m]) * 60",
          "legendFormat": "Output Tokens/min"
        }
      ]
    },
    {
      "title": "Cost Over Time",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "increase(conductor_cost_total_dollars[1h])",
          "legendFormat": "Cost/hour"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        }
      }
    },
    {
      "title": "Agent Runs by Type",
      "type": "piechart",
      "gridPos": { "x": 0, "y": 20, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "conductor_agent_runs_total",
          "legendFormat": "{{agent_type}}"
        }
      ]
    },
    {
      "title": "Agent Run Rate",
      "type": "timeseries",
      "gridPos": { "x": 8, "y": 20, "w": 16, "h": 8 },
      "targets": [
        {
          "expr": "rate(conductor_agent_runs_total[5m]) * 60",
          "legendFormat": "{{agent_type}}/min"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["conductor", "automation", "claude"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Conductor Dashboard",
  "uid": "conductor-main",
  "version": 1,
  "weekStart": ""
}
